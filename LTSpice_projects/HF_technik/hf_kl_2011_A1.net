L1 0 0 154n
C1 0 0 103p
I1 0 out SINE(0 {sqrt(2)*1m} {fstim}) AC 1m
R2 0 0 1meg
R3 0 0 1meg
L2 out 0 {L}
C2 out 0 {C}
R1 out 0 {Rpl}
R4 out 0 {Rpc}
R5 out 0 {Rg}
XU1 Rpc vdc_single V={Rpc}
XU2 Rpl vdc_single V={Rpl}
XU3 w0 vdc_single V={w0}
I2 0 out2 SINE(0 {sqrt(2)*1m} {fstim}) AC 1m
