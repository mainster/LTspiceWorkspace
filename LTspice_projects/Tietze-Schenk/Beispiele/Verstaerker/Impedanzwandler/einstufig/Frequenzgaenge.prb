[DISPLAYS]
BEGIN DISPLAY Frequenzgaenge
ANALYSIS ALL_AC_SWEEP
SYMBOL ALWAYS
TRACECOLOR NORMAL
BEGIN ANAPLOT 1
ACTIVE
XBASE
BEGIN XAXIS
TEXT Frequency
RANGEFLAG AUTO
TYPE LOG
UNIT H
END XAXIS
BEGIN YAXIS 1
ACTIVE
RANGEFLAG MANUAL
TYPE LINEAR
UNIT  
REFMIN -20
REFMAX 5
REFUNIT  
TRACE dB(V(Ua1))
TRACE dB(V(Ua2))
END YAXIS 1
END ANAPLOT 1
END DISPLAY Frequenzgaenge
