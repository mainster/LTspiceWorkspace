BEGIN DISPLAY Eingangsimpedanzen
ANALYSIS AC_SWEEP
SYMBOL ALWAYS
TRACECOLOR NORMAL
BEGIN ANAPLOT 1
ACTIVE
XBASE
BEGIN XAXIS
TEXT Frequency
RANGEFLAG AUTO
TYPE LOG
UNIT H
END XAXIS
BEGIN YAXIS 1
RANGEFLAG MANUAL
TYPE LOG
UNIT x
REFMIN 10
REFMAX 10000
REFUNIT  
TRACE V(Ue1)/IB(T1)
TRACE V(Ue2)/IB(T2)
END YAXIS 1
BEGIN YAXIS 2
ACTIVE
RANGEFLAG MANUAL
TYPE LINEAR
UNIT d
REFMIN -90
REFMAX 0
REFUNIT  
TRACE P(V(Ue1)/IB(T1))
TRACE P(V(Ue2)/IB(T1))
END YAXIS 2
END ANAPLOT 1
END DISPLAY Eingangsimpedanzen
BEGIN DISPLAY re_Ce
ANALYSIS AC_SWEEP
SYMBOL ALWAYS
TRACECOLOR NORMAL
BEGIN ANAPLOT 1
ACTIVE
XBASE
BEGIN XAXIS
TEXT Frequency
RANGEFLAG AUTO
TYPE LOG
UNIT H
END XAXIS
BEGIN YAXIS 1
ACTIVE
RANGEFLAG MANUAL
TYPE LINEAR
UNIT x
REFMIN 0
REFMAX 5000
REFUNIT  
TRACE 1/R(IB(T1)/V(Ue1))
TRACE 1/R(IB(T2)/V(Ue2))
END YAXIS 1
BEGIN YAXIS 2
RANGEFLAG MANUAL
TYPE LINEAR
UNIT x
REFMIN 0
REFMAX 5e-010
REFUNIT  
TRACE IMG(IB(T1)/V(Ue1))/(6.28*Frequency)
TRACE IMG(IB(T2)/V(Ue2))/(6.28*Frequency)
END YAXIS 2
END ANAPLOT 1
END DISPLAY re_Ce
