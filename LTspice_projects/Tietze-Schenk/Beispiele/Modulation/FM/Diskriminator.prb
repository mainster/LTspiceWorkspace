BEGIN DISPLAY Frequenzgang
ANALYSIS AC_SWEEP
SYMBOL AUTO
TRACECOLOR NORMAL
BEGIN ANAPLOT 1
ACTIVE
XBASE
BEGIN XAXIS
TEXT Frequency
RANGEFLAG MANUAL
TYPE LINEAR
UNIT H
REFMIN 8000000
REFMAX 13000000
REFUNIT H
END XAXIS
BEGIN YAXIS 1
RANGEFLAG MANUAL
TYPE LINEAR
UNIT V
REFMIN -0.8
REFMAX 1
REFUNIT V
TRACE abs(V(U1))
TRACE abs(V(U2))
TRACE abs(V(U2))-abs(V(U1))
END YAXIS 1
BEGIN YAXIS 2
ACTIVE
RANGEFLAG MANUAL
TYPE LINEAR
UNIT  
REFMIN -1.6e-006
REFMAX 2e-006
REFUNIT  
TRACE d(abs(V(U1)))
TRACE d(abs(V(U2))-abs(V(U1)))
END YAXIS 2
END ANAPLOT 1
END DISPLAY Frequenzgang
