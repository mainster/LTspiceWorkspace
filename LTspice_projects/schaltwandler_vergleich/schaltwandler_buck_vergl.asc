TEXT 1400 -408 Left 2 !; ---------------------------------------------------\n; Tastverhältniss\n; ---------------------------------------------------\n.MEAS TRAN t0 FIND time*1e6 WHEN I(L1)=I(I1) cross=1 TD=1u\n.MEAS TRAN t1 FIND time*1e6 WHEN I(L1)=I(I1) cross=2 TD=1u\n.MEAS TRAN t2 FIND time*1e6 WHEN I(L1)=I(I1) cross=3 TD=1u\n.MEAS TRAN trise FIND time WHEN I(L1)=I(I1) rise=1 TD=1u\n.MEAS TRAN tfall FIND time WHEN I(L1)=I(I1) fall=1 TD=1u\n \n.MEAS TRAN T PARAM round(t2-t0)\n.MEAS TRAN ton PARAM round(t1-t0)\n \n; ---------------------------------------------------\n; Rippelspannung berechnen\n; ---------------------------------------------------\n.MEAS TRAN area_ICa INTEG I(CA)\n+ TRIG I(CA)=0 RISE=1\n+ TARG I(CA)=0 FALL=1\n \n.MEAS TRAN vrip PARAM area_ICa/CA\n \n; ---------------------------------------------------\n; Rippelspannung direkt messen\n; ---------------------------------------------------\n.MEAS TRAN vpp pp V(A) \n+ TRIG time=0\n+ TARG time=300u\n \n; ---------------------------------------------------\n; ESR\n; ---------------------------------------------------\n.MEAS TRAN ILmax MAX I(L1) TD=10u\n.MEAS TRAN ILmin MIN I(L1) TD=10u\n.MEAS TRAN p1 FIND V(a,ca) WHEN I(L1)=ILmin fall=last\n.MEAS TRAN p2 FIND V(a,ca) WHEN I(L1)=ILmax rise=last\n.MEAS TRAN dVa PARAM abs(p2-p1)\n.MEAS TRAN dIL PARAM abs(ILmax-ILmin)\n.MEAS TRAN Rser PARAM dVa/dIL\n \n; ---------------------------------------------------\n; Model defines\n; ---------------------------------------------------\n.model swL SW(Ron=1m Roff=100Meg Vt=0 Vh=-.5 )\n.model swT SW(Ron=1m Roff=100meg Vt=0 Vh=-.5 )\n.model Dx D (Vfwd = {Vd0} Ron = 10m Roff 100meg)\n.model Dx2 D (Is={Ia/exp(Vd0/(1.1*26m))} N=1.1)
